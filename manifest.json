{
	"name": "LinkfireAngular",
	"version": "1.0",
	"manifest_version": 2,

	"description": "Linkfire extensions with AngularJS",
	"icons": {
		"128": "icon128.png"
	},
	"browser_action": {
		"default_icon": "img/defaultIcon19x19.png",
		"default_popup": "popup.html",
		"default_title": "AngularJSChromeEx"
	},
	"minimum_chrome_version": "18",
	"permissions": [ "http://*/*", "https://*/*", "storage", "contextMenus", "cookies", "tabs", "notifications" ]

}