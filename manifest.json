{
	"name": "Linkfire Chrome Extension",
	"version": "0.1",
	"manifest_version": 2,

	"description": "Linkfire extensions with AngularJS",
	"icons": {
		"128": "icon128.png"
	},
		"content_scripts": [ {
	"js": [ "js/lib/jquery-2.1.0.min.js", "js/app/content.js" ],
	"matches": [ "*://*/*" ],
	"run_at": "document_start"
	} ],
	"browser_action": {
		"default_icon": "img/defaultIcon19x19.png",
		"default_popup": "popup.html",
		"default_title": "AngularJSChromeEx"
	},
	"minimum_chrome_version": "18",
	"permissions": [ "http://*/*", "storage", "tabs"]

}